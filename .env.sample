# Use these values to connect to the local database
POSTGRES_HOST=localhost
POSTGRES_USERNAME=chatlse
POSTGRES_PASSWORD=chatlse
POSTGRES_DATABASE=chatlse
POSTGRES_SSL=disable
POSTGRES_PORT=5432

# API_HOST can be either ollama, or openai:
OPENAI_API_HOST=ollama 
OPENAI_EMBED_HOST=ollama

# Needed for OpenAI.com:
OPENAICOM_KEY=<YOUR-OPENAI-KEY>
OPENAICOM_CHAT_MODEL=gpt-3.5-turbo
OPENAICOM_EMBED_MODEL=text-embedding-ada-002
OPENAICOM_EMBED_MODEL_DIMENSIONS=1536

# Needed for Ollama:
OLLAMA_ENDPOINT=http://localhost:11434/v1
OLLAMA_CHAT_MODEL=llama3:instruct
OLLAMA_EMBED_MODEL=thenlper/gte-large

